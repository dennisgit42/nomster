<div class="booyah-box col-10 offset-1">
  <h1>Edit Your Place</h1>
  <%= simple_form_for @place do |f| %>
    <%= f.input :name ,class:"tether-theme-basic"%>
    <%= f.input :address %>
    <%= f.input :description %>
    <br />
    <%= f.submit "Update", class:"btn btn-primary"%>
  <% end %>
</div>